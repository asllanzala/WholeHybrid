<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="64dp">

    <ImageView
        android:id="@+id/search_result_supplier_imageView"
        android:layout_width="52dp"
        android:layout_height="52dp"
        android:layout_centerVertical="true"
        android:layout_marginStart="14dp"
        />

    <com.honeywell.wholesale.ui.base.BaseTextView
        android:id="@+id/search_result_supplierName_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@id/search_result_supplier_imageView"
        android:layout_marginStart="10dp"
        android:layout_centerVertical="true"
        android:maxLines="1"
        android:maxWidth="100dp"
        android:textSize="14dp"
        android:text="123123123"
        android:textColor="@color/gray_deep"
        />

    <com.honeywell.wholesale.ui.base.BaseTextView
        android:id="@+id/search_result_supplier_tele_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="10dp"
        android:layout_marginStart="10dp"
        android:layout_centerVertical="true"
        android:maxLines="1"
        android:maxWidth="120dp"
        android:textSize="14dp"
        android:text="1231231333323"
        android:textColor="@color/gray_deep"
        />

</RelativeLayout>